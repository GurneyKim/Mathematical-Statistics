proc iml;
    do x = 1 to 100;
    do y = 1 to x;
    do z = 1 to y;
      temp = x*x*x + y*y*y + z*z*z;
      if (abs(temp-2002) < 0.1) then print "+++" x y z;
      temp = x*x*x - y*y*y + z*z*z;
      if (abs(temp-2002) < 0.1) then print "+-+" x y z;
      temp = x*x*x + y*y*y - z*z*z;
      if (abs(temp-2002) < 0.1) then print "++-" x y z;
      temp = x*x*x - y*y*y - z*z*z;
      if (abs(temp-2002) < 0.1) then print "+--" x y z;
    end;  end;  end;
quit;
